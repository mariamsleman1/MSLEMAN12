<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/backroung"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ofwhite"
    android:backgroundTintMode="multiply"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Coustomer"

        android:layout_width="157dp"
        android:layout_height="61dp"
        android:layout_marginTop="313dp"
        android:layout_marginBottom="67dp"
        android:backgroundTint="@color/dahabe"
        android:onClick="coustumer"
        android:text="Coustumer"
        app:layout_constraintBottom_toTopOf="@+id/btnemployee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnmanager"
        android:onClick="maneger"
        android:layout_width="174dp"
        android:layout_height="65dp"
        android:layout_marginBottom="92dp"
        android:backgroundTint="@color/dahabe"
        android:text="manager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnemployee" />

    <Button
        android:id="@+id/btnemployee"
        android:onClick="employee"
        android:layout_width="179dp"
        android:layout_height="60dp"
        android:layout_marginBottom="75dp"
        android:backgroundTint="@color/dahabe"
        android:text="employee"
        app:layout_constraintBottom_toTopOf="@+id/btnmanager"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Coustomer" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="286dp"
        android:layout_height="215dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toTopOf="@+id/Coustomer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/project" />

</androidx.constraintlayout.widget.ConstraintLayout>