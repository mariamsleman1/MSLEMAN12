<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ofwhite"
    tools:context=".menumealsActivity">

    <Button
        android:id="@+id/breakfastbittonmenu"
        android:onClick="breakfastbtn"
        android:layout_width="275dp"
        android:layout_height="85dp"
        android:layout_marginBottom="422dp"
        android:backgroundTint="@color/dahabe"
        android:text="breakfast"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <Button
        android:id="@+id/lunchbuttonmenu"
        android:onClick="lunchbtn"
        android:layout_width="281dp"
        android:layout_height="74dp"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="307dp"
        android:backgroundTint="@color/dahabe"
        android:text="lunch"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.862" />

    <Button
        android:id="@+id/dinnerbuttonmenu"
        android:onClick="dinnerbtn"
        android:layout_width="283dp"
        android:layout_height="74dp"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="232dp"
        android:backgroundTint="@color/dahabe"
        android:text="dinner"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/dessertbuttonmenu"
        android:onClick="dessertbtn"
        android:layout_width="274dp"
        android:layout_height="74dp"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        android:layout_marginBottom="127dp"
        android:backgroundTint="@color/dahabe"
        android:text="dessert"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <Button
        android:id="@+id/drinkbuttonmenu"
        android:onClick="drinkbtn"
        android:layout_width="273dp"
        android:layout_height="67dp"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="42dp"
        android:backgroundTint="@color/dahabe"
        android:text="drinks"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="357dp"
        app:layout_constraintBottom_toTopOf="@+id/dessertbuttonmenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/project" />

    <FrameLayout
        android:id="@+id/menuframelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>